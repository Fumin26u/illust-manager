{
    "name": "aicv_webui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run serve\" \"npm run start-flask\"",
        "serve": "vue-cli-service serve --host 0.0.0.0",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "start-flask": "set FLASK_APP=app.py && python -m flask run --reload"
    },
    "dependencies": {
        "axios": "^1.6.2",
        "concurrently": "^8.2.2",
        "pinia": "^2.1.7",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "uuid": "^9.0.1",
        "vue": "^3.2.13",
        "vue-axios": "^3.5.2",
        "vue-router": "^4.0.3",
        "vuetify": "^3.6.14"
    },
    "devDependencies": {
        "@mdi/font": "^7.4.47",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-plugin-router": "~5.0.0",
        "@vue/cli-plugin-typescript": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.0.3",
        "prettier": "^2.4.1",
        "typescript": "^5.3.3"
    }
}
